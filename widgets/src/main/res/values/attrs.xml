<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LSettingView">
        <attr name="leftText" format="string" />
        <attr name="leftIcon" format="integer" />
        <attr name="rightIcon" format="integer" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="isShowUnderLine" format="boolean" />
        <attr name="leftIconSize" format="dimension" />
        <attr name="leftTextMarginLeft" format="dimension" />
        <attr name="rightStyle" format="enum">
            <enum name="iconShow" value="0" />
            <enum name="iconHide" value="1" />
            <enum name="iconCheck" value="2" />
            <enum name="iconSwitch" value="3" />
            <enum name="iconGone" value="4" />
        </attr>
        <attr name="isShowRightText" format="boolean" />
        <attr name="rightText" format="string" />
        <attr name="rightTextSize" format="dimension" />
        <attr name="rightTextColor" format="color" />
    </declare-styleable>


    <declare-styleable name="LEditTextItem">


        <attr name="showicon" format="boolean" />
        <attr name="title" format="string" />
        <attr name="title_size" format="dimension" />
        <attr name="title_color" format="color"/>
        <attr name="content" format="string" />
        <attr name="content_size" format="dimension" />
        <attr name="content_color" format="color" />
        <attr name="hintcontent" format="string" />
        <attr name="hintcontent_color" format="color" />
        <attr name="isedit" format="boolean" />


    </declare-styleable>
</resources>